import clase01.pepita.pepita.*

test "Entrenador entrena a pepita y cambia energia" {
	entrenador.entrenar(pepita)
	assert.equals(0, pepita.energia())
}

test "Entrenador entrena a josefa y la hace volar" {
	entrenador.entrenar(josefa)
	assert.that(josefa.volo())
}

test "Entrenador entrena a josefa y la hace comer" {
	entrenador.entrenar(josefa)
	assert.that(josefa.comio())
}