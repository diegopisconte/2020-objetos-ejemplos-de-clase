import example.*

test "Un producto sin stock tira una excepcion al validarStock" {
	const silla = new Producto(nombre="silla", stock=0, precio=1500)
	const sillaPorComprar = new ProductoAgregado(producto=silla, cantidad=1)


	assert.throwsExceptionWithType(new OutOfStockException(),
								   { sillaPorComprar.validarEstoc() }
	)
}
